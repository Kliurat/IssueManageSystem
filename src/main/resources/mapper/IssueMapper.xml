<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- mapper标签要指定namespace属性，不然会报错，可看做包名-->
<mapper namespace="com.ibm.springboot.dao.IssueDao">

	<select id="findAll" resultType="com.ibm.springboot.entity.Issue">
		select 
			id,issue_no,issue_type,create_date,plan_modify_time
			from issue
	</select>
	
	<insert	id="insertIssue">
		insert into issue(
		issue_no,issue_type,create_date,plan_modify_time,
		actual_complte_time,priority,influent_version,
		re_step,create_person_ID,modify_person_ID,title,status) values (
			#{issue.issueNo},
			#{issue.issueType},
			#{issue.createDate},
			#{issue.planModifyTime},
			#{issue.actualComplteTime},
			#{issue.priority},
			#{issue.influentVersion},
			#{issue.reStep},
			#{issue.createPersonID},
			#{issue.modifyPersonID},
			#{issue.title},
			#{issue.status},
		)
		
	</insert>
</mapper>